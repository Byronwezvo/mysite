# .gitignore file for a Django project

# Ignore virtual environment
venv/

# Ignore database file
db.sqlite3

# Ignore compiled Python files
__pycache__/

# Ignore static files generated by collectstatic
staticfiles/

# Ignore media files generated during development
media/

# Ignore local settings (for example, settings/local.py)
local_settings.py

# Ignore logs
*.log

# Ignore environment configuration files
.env

# Ignore Python bytecode files
*.pyc
*.pyo
*.pyd

# Ignore macOS system files
.DS_Store

# Ignore editor-specific files
.vscode/
.idea/
*.suo
*.ntvs*

# Ignore dependencies
node_modules/
